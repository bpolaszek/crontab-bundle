{
    "name": "bentools/crontab-bundle",
    "description": "Generates a crontab based on the app directory.",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.0|^5.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/process": "^4.0|^5.0",
        "symfony/yaml": "^4.0|^5.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.0|^5.0",
        "pestphp/pest": "^0.3.14",
        "symfony/http-kernel": "^4.0|^5.0",
        "thecodingmachine/safe": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "BenTools\\CrontabBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenTools\\CrontabBundle\\Tests\\SampleApp\\": "tests/app",
            "BenTools\\CrontabBundle\\Tests\\": "tests"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
